
public class Factorize {
	
	public static void printfact(int num){
        int temp=0;
        for(int i=num-1;i>=num/2;i--){
            if(num % i == 0){
                temp = num/i;
                System.out.println(temp + " * " + i);
            }
        }
}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Factorize.printfact(12);

	}

}
